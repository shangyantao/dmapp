<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false">

	<diskStore path="java.io.tmpdir/hibernate/data-app" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600"
		overflowToDisk="true" maxEntriesLocalDisk="100000" />

	<!-- Special objects setting. -->
	<cache name="com.sap.data.app.entity.account.User" maxEntriesLocalHeap="1000" eternal="true"
		overflowToDisk="true" maxEntriesLocalDisk="10000" />
	<cache name="com.sap.data.app.entity.account.User.groupList" maxEntriesLocalHeap="1000" eternal="true"
		overflowToDisk="true" maxEntriesLocalDisk="10000" />
	<cache name="com.sap.data.app.entity.account.Group" maxEntriesLocalHeap="100" eternal="true"
		overflowToDisk="true" maxEntriesLocalDisk="1000" />
